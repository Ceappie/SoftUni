
public class P4_MagicCarNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] digitsWeight = {10, 20, 30, 50, 80, 110, 130, 160, 200, 240};
		
		for (int a = 0; a < 10; a++) {
			for (int b = 0; b < 10; b++) {
				for (int x = 0; x < 10; x++) {
					for (int y = 0; y < 10; y++) {
						int currentWeight = getWeight(a, a, a, a, digitsWeight[x], digitsWeight[y]);
					
					}
				}
			}
		}
	}

	private static int getWeight(int d1, int d2, int d3, int d4, int d5, int d6) {
		int wieght = d1 + d2 + d3 + d4 + d5 + d6;
		return 0;
	}

}
